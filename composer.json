{
    "name": "chmez/d8",
    "description": "Profile for Drupal 8/9.",
    "type": "drupal-profile",
    "license": "GPL-2.0-or-later",
    "require": {
        "composer/installers": "1.11.0",
        "cweagans/composer-patches": "^1.0",
        "drupal/admin_toolbar": "3.3.0",
        "drupal/autoban": "1.9",
        "drupal/bootstrap": "3.24",
        "drupal/captcha": "1.9",
        "drupal/config_update": "1.7",
        "drupal/core": "9.5.9",
        "drupal/core-composer-scaffold": "9.5.9",
        "drupal/features": "3.12",
        "drupal/module-filter": "4.0.1",
        "drupal/recaptcha": "3.1",
        "drupal/recaptcha_preloader": "1.0.1",
        "drupal/standwithukraine": "1.3.9"
    },
    "require-dev": {
        "drupal/config2php": "2.0.2",
        "drupal/upgrade_status": "3.17"
    },
    "extra": {
        "patches": {
            "drupal/config_override": {
                "PHP Fatal error: Uncaught Error: Call to a member function getParameter() on null": "https://www.drupal.org/files/issues/2021-08-03/call_to_member_function_getparameter_3226418_1.patch"
            },
            "drupal/core": {
                "Default theme favicon for administration theme": "https://www.drupal.org/files/issues/2019-12-14/default-theme-favicon-for-administration-theme-2981434-7.patch",
                "Add events for matching entity hooks": "https://www.drupal.org/files/issues/2022-12-22/2551893-99.patch",
                "News mode": "https://www.drupal.org/files/issues/2022-06-20/drupal-news-mode-3256060-10.patch"
            },
            "drupal/features": {
                "Set default bundle for site": "https://www.drupal.org/files/issues/2022-01-08/features-current-bundle-2789001-14.patch"
            },
            "drupal/recaptcha": {
                "Integrate Invisible reCAPTCHA option from Google": "https://www.drupal.org/files/issues/2022-10-12/recaptcha-ajax-2493183-164%2Bnocaptcha-2852269-203.patch",
                "Register in the new tab": "https://www.drupal.org/files/issues/2023-03-05/recaptcha-register-in-the-new-tab-3346065-2.patch"
            }
        },
        "enable-patching": true,
        "installer-paths": {
            "modules/contrib/{$name}": [ "type:drupal-module" ],
            "themes/contrib/{$name}": [ "type:drupal-theme" ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ]
}

{
    "name": "lexhouk/d8",
    "description": "Profile for Drupal 10.",
    "type": "drupal-profile",
    "license": "GPL-2.0-or-later",
    "require": {
        "composer/installers": "1.11.0",
        "cweagans/composer-patches": "^1.0",
        "drupal/admin_toolbar": "3.4.1",
        "drupal/autoban": "1.9",
        "drupal/bootstrap": "3.29",
        "drupal/captcha": "1.9",
        "drupal/config_update": "2.0.0-alpha3",
        "drupal/core": "10.1.5",
        "drupal/core-composer-scaffold": "10.1.5",
        "drupal/exception_mailer": "4.0.0",
        "drupal/features": "3.13",
        "drupal/module_filter": "4.0.1",
        "drupal/recaptcha": "3.1",
        "drupal/recaptcha_preloader": "1.0.1",
        "drupal/seven": "1.0.0",
        "drupal/standwithukraine": "1.3.10",
        "drupal/symfony_mailer": "1.3.2"
    },
    "require-dev": {
        "drupal/config2php": "2.1.0",
        "twbs/bootstrap-sass": "3.4.3"
    },
    "extra": {
        "patches": {
            "drupal/autoban": {
                "Mixed links": "https://www.drupal.org/files/issues/2023-09-09/autoban-mixed-links-3386212-2.patch"
            },
            "drupal/core": {
                "Default theme favicon for administration theme": "https://www.drupal.org/files/issues/2019-12-14/default-theme-favicon-for-administration-theme-2981434-7.patch",
                "Add events for matching entity hooks": "https://www.drupal.org/files/issues/2022-12-22/2551893-99.patch",
                "News mode": "https://www.drupal.org/files/issues/2022-06-20/drupal-news-mode-3256060-10.patch"
            },
            "drupal/exception_mailer": {
                "Queue workers are broken": "https://www.drupal.org/files/issues/2023-10-20/exception-mailer-queue-worker-parent-3395572-2.patch"
            },
            "drupal/features": {
                "Set default bundle for site": "https://www.drupal.org/files/issues/2023-07-03/features-current-bundle-2789001-17.patch",
                "Deprecated function: Creation of dynamic property Drupal\\features\\FeaturesGenerationMethodManager::$cacheKeyPrefix is deprecated in Drupal\\features\\FeaturesGenerationMethodManager->__construct() (line 28 of modules/contrib/features/src/FeaturesGenerationM": "https://www.drupal.org/files/issues/2023-07-19/3375425-3.patch"
            },
            "drupal/module_filter": {
                "Missing autocomplete library": "https://www.drupal.org/files/issues/2023-06-30/module-filter-missing-autocomplete-library-3371613-3.patch"
            },
            "drupal/recaptcha": {
                "Integrate Invisible reCAPTCHA option from Google": "https://www.drupal.org/files/issues/2022-10-12/recaptcha-ajax-2493183-164%2Bnocaptcha-2852269-203.patch",
                "Register in the new tab": "https://www.drupal.org/files/issues/2023-03-05/recaptcha-register-in-the-new-tab-3346065-2.patch"
            },
            "drupal/symfony_mailer": {
                "Add support for complex address strings in back-compatibility mode": "https://www.drupal.org/files/issues/2023-03-30/symfony_mailer-support_complex_address_strings-3350992-3.patch",
                "Drupal 10.1.0 new aggregation breaks InlineCssEmailAdjuster": "https://www.drupal.org/files/issues/2023-09-21/symfony_mailer.inline-css.3371042-88.patch"
            }
        },
        "enable-patching": true,
        "installer-paths": {
            "modules/contrib/{$name}": [ "type:drupal-module" ],
            "themes/contrib/{$name}": [ "type:drupal-theme" ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ]
}
